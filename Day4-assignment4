4..  Identify the error / output of the program
#include<bits/stdc++.h>
usingnamespacestd;
 
intmain()
{
   intx = 10;
   inty = seventy;
 
   x = x + y;
   y = x - y;
   x = x - y;
 
   cout<< "X : "<< x << "\n";
   cout<< "Y : "<< y << "\n";
    
   return0;
}
solution:
#include<iostream>
using namespace std;
 
int main()
{
   int x = 10;
   int y = 70;
 
   x = x + y;
   y = x - y;
   x = x - y;
 
   cout<< "X : "<< x << "\n";
   cout<< "Y : "<< y << "\n";
    
   return 0;
}

